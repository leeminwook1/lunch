# MongoDB 연결 문자열
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/lunch-menu-picker?retryWrites=true&w=majority

# 환경 설정
NODE_ENV=development

# 관리자 비밀번호 (선택사항, 기본값: 123)
ADMIN_PASSWORD=123

# API 레이트 리미팅 설정 (선택사항)
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000

# 사용자당 최대 가게 수 (선택사항)
MAX_RESTAURANTS_PER_USER=50

# Next.js 설정
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here